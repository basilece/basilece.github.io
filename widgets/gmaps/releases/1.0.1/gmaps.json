		"returnType": "string",
		"description": "Returning the Text",
		"parameters": [],
		"body": "return this.TextAdressLong;"
      } ,
      
      
 	"getGeolocationLat": {
		"returnType": "string",
		"description": "Returning the Text",
		"parameters": [],
		"body": "return this.TextAdressLat;"
      }    
        
        
        

	},
	"events": {
